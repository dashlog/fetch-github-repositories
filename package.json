{
  "name": "fetch-github-repositories",
  "version": "2.0.0",
  "description": "Fetch github repositories for a given user (or organization)",
  "main": "index.js",
  "scripts": {
    "test": "cross-env eslint index.js && jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraxken/fetch-github-repositories.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/"
    ]
  },
  "keywords": [
    "retrieve",
    "fetch",
    "git",
    "github",
    "repositories",
    "repo",
    "repos",
    "api",
    "user",
    "org",
    "organization",
    "list",
    "api",
    "request"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fraxken/fetch-github-repositories/issues"
  },
  "homepage": "https://github.com/fraxken/fetch-github-repositories#readme",
  "devDependencies": {
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/is": "^1.5.1",
    "cross-env": "^7.0.3",
    "jest": "^27.3.1",
    "repos": "^2.0.0"
  },
  "dependencies": {
    "combine-async-iterators": "^2.0.1",
    "http-link-header": "^1.0.3",
    "httpie": "^1.1.2"
